{
  "name": "snyk",
  "description": "snyk library and cli utility",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "bin": {
    "snyk": "./cli/index.js"
  },
  "scripts": {
    "test": "jscs cli/*.js cli/**/*.js lib/*.js -v && tape test/*.test.js --cov | tap-spec",
    "watch": "nodemon -q -x 'for FILE in test/*.test.js; do echo $FILE; tape $FILE | tap-spec; if [ $? -ne 0 ]; then exit 1; fi; done'",
    "//semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "keywords": [
    "security",
    "snyk"
  ],
  "private": true,
  "author": "Remy Sharp",
  "license": "UNLICENSED",
  "dependencies": {
    "@snyk/config": "^3.0.0",
    "configstore": "^1.2.0",
    "debug": "^2.2.0",
    "es6-promise": "^2.3.0",
    "lodash.foreach": "^3.0.3",
    "minimist": "^1.1.1",
    "promise": "^7.0.3",
    "request": "^2.60.0",
    "semver": "^5.0.1",
    "then-fs": "^2.0.0"
  },
  "devDependencies": {
    "@remy/snyk-registry": "^1.0.0",
    "jscs": "^2.0.0",
    "nodemon": "^1.3.7",
    "tap-spec": "^4.0.2",
    "tape": "^4.0.0",
    "semantic-release": "^4.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Snyk/snyk.git"
  }
}
