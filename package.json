{
  "name": "@snyk/snyk",
  "description": "snyk library and cli utility",
  "main": "lib/index.js",
  "version": "0.0.0",
  "directories": {
    "test": "test"
  },
  "bin": {
    "snyk": "./cli/index.js"
  },
  "scripts": {
    "lint": "jscs cli/*.js cli/**/*.js lib/*.js -v",
    "test": "npm run lint && tape test/*.test.js --cov | tap-spec",
    "watch": "nodemon -q -x 'for FILE in test/*.test.js; do echo $FILE; tape $FILE | tap-spec; if [ $? -ne 0 ]; then exit 1; fi; done'",
    "//semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "keywords": [
    "security",
    "snyk"
  ],
  "author": "Remy Sharp",
  "license": "UNLICENSED",
  "dependencies": {
    "@snyk/config": "^3.0.0",
    "@snyk/module": "^1.0.0",
    "abbrev": "^1.0.7",
    "base32": "0.0.6",
    "configstore": "^1.2.0",
    "debug": "^2.2.0",
    "es6-promise": "^3.0.2",
    "inquirer": "^0.10.0",
    "js-yaml": "^3.4.2",
    "lodash": "^3.10.1",
    "minimist": "^1.1.1",
    "npmlog": "^1.2.1",
    "promise": "^7.0.3",
    "request": "^2.60.0",
    "semver": "^5.0.1",
    "then-fs": "^2.0.0",
    "validator": "^4.0.5"
  },
  "optionalDependencies": {
    "keytar": "^3.0.0"
  },
  "devDependencies": {
    "@snyk/registry": "^1.4.0",
    "jscs": "^2.0.0",
    "nodemon": "^1.3.7",
    "semantic-release": "^4.1.0",
    "tap-spec": "^2.2.2",
    "tape": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Snyk/snyk.git"
  }
}
